<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="NHibernateCourse.QuickStart.Model"
                   assembly="NHibernateCourse.QuickStart"
                   >
  <class name="File" table="Files">
    <id name="Id">
      <generator class="identity"/>
    </id>
    <property name="Name"/>
   
  </class>

  <class name="Graphic">
    <id name="Id">
      <generator class="identity"/>
    </id>
    <property name="Name"/>
    <property name="Type"/>
  </class>

  <class name="Final">
    <id name="Id">
      <generator class="identity"/>
    </id>

    <property name="Name"/>
    <property name="Type"/>
    <property name="ColorDepth"/>
  </class>
  
  <class name="Rough">
    <id name="Id">
      <generator class="identity"/>
    </id>
    <property name="Name"/>
    <property name="Type"/>

    <property name="Height"/>
    <property name="Width"/>
  </class>



</hibernate-mapping>